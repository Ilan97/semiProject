<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.AlertTimeIsRunningOutWindowController">
   <children>
      <Pane prefHeight="255.0" prefWidth="353.0" style="-fx-background-color: #ffffff;">
         <children>
            <ImageView fx:id="imgCrash" fitHeight="255.0" fitWidth="353.0" layoutY="-1.0" />
            <Text fill="#f86060" layoutX="29.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Time Running Out" textAlignment="CENTER" wrappingWidth="289.0">
               <font>
                  <Font name="Calibri Bold" size="36.0" />
               </font>
            </Text>
            <Button fx:id="btnApprove" layoutX="149.0" layoutY="184.0" mnemonicParsing="false" onAction="#okAction" prefHeight="27.0" prefWidth="60.0" style="-fx-background-color: #b2b2b2; -fx-background-radius: 30 30 30 30;" text="Ok" textFill="WHITE">
               <font>
                  <Font name="Calibri Bold" size="14.0" />
               </font>
            </Button>
            <Label layoutX="45.0" layoutY="77.0" prefHeight="89.0" prefWidth="182.0" text="You have 10 minutes left to complete the exam" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Calibri Bold" size="18.0" />
               </font>
            </Label>
            <ImageView fx:id="imgTimer" fitHeight="70.0" fitWidth="70.0" layoutX="230.0" layoutY="87.0" />
         </children>
      </Pane>
   </children>
</AnchorPane>
