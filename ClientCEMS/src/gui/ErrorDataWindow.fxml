<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.ErrorDataWindowController">
   <children>
      <Pane prefHeight="242.0" prefWidth="228.0" style="-fx-background-color: #ffffff;">
         <children>
            <ImageView fx:id="imgBack" fitHeight="242.0" fitWidth="228.0">
               <image>
                  <Image url="@../../../../photos/serverCrushed.PNG" />
               </image>
            </ImageView>
            <Text fill="#f86060" layoutX="61.0" layoutY="58.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Message Alert" textAlignment="CENTER" wrappingWidth="140.13671827316284">
               <font>
                  <Font name="Calibri Bold" size="23.0" />
               </font>
            </Text>
            <Button layoutX="46.0" layoutY="180.0" mnemonicParsing="false" onAction="#okAction" prefHeight="27.0" prefWidth="55.0" style="-fx-background-color: #f86060; -fx-background-radius: 30 30 30 30;" text="Ok" textFill="WHITE">
