<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ServerPortController">
   <children>
      <Pane prefHeight="260.0" prefWidth="254.0" style="-fx-background-color: #ffffff;">
         <children>
            <ImageView fx:id="imgPort" fitHeight="254.0" fitWidth="243.0" layoutX="5.0" layoutY="3.0" />
            <Text fill="#0070c0" layoutX="94.0" layoutY="69.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Port" wrappingWidth="88.13671875">
               <font>
                  <Font name="Calibri Bold" size="36.0" />
               </font>
            </Text>
            <Button fx:id="btnConnect" layoutX="98.0" layoutY="187.0" mnemonicParsing="false" onAction="#connectActionButton" style="-fx-background-color: #0070C0; -fx-background-radius: 30 30 30 30;" text="connect" textFill="WHITE">
               <font>
                  <Font name="Calibri Bold" size="14.0" />
               </font>
            </Button>
            <TextField fx:id="txtPort" layoutX="89.0" layoutY="119.0" onKeyPressed="#inputPass" prefHeight="28.0" prefWidth="79.0">
               <font>
                  <Font name="Calibri" size="12.0" />
               </font></TextField>
            <Text fontSmoothingType="LCD" layoutX="98.0" layoutY="113.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Port ">
               <font>
                  <Font name="Calibri Bold" size="14.0" />
               </font>
            </Text>
            <Label fx:id="lblErrorPort" alignment="CENTER" layoutX="34.0" layoutY="69.0" prefHeight="17.0" prefWidth="189.0" textFill="#f86060">
               <font>
                  <Font name="Calibri Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
